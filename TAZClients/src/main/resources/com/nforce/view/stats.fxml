        <VBox AnchorPane.bottomAnchor="10" AnchorPane.topAnchor="10" AnchorPane.leftAnchor="10" AnchorPane.rightAnchor="10">
           <children>
              <TitledPane animated="false" collapsible="false" text="Prenumeratoriai">
                <content>
                  <AnchorPane>
                      <children>
                          <TableView fx:id="subscribersTable" AnchorPane.bottomAnchor="10" AnchorPane.topAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" />
                      </children>
                  </AnchorPane>
                </content>
              </TitledPane>
              <TitledPane animated="false" collapsible="false" text="Galiojanti prenumerata">
                <content>
                  <AnchorPane>
                      <children>
                          <GridPane fx:id="activeSubscribers" alignment="CENTER_LEFT" hgap="15" vgap="15" AnchorPane.bottomAnchor="10" AnchorPane.topAnchor="10" AnchorPane.leftAnchor="10" AnchorPane.rightAnchor="10" />
                      </children>
                  </AnchorPane>
                </content>
              </TitledPane>
           </children>
        </VBox>
    </children>
</AnchorPane>
